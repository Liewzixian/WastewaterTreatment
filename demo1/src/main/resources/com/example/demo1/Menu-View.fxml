<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.MenuController">
   <children>
      <ImageView fitHeight="292.0" fitWidth="780.0" layoutX="8.0" layoutY="94.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../wastewater/menuBackground.png" />
         </image>
      </ImageView>
      <Button fx:id="addButton" layoutX="218.0" layoutY="187.0" mnemonicParsing="false" onAction="#addButtonOnAction" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #00D4FF;" text="Add New Model" />
      <Button fx:id="ManageButton" layoutX="218.0" layoutY="227.0" mnemonicParsing="false" onAction="#manageButtonOnAction" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #00D4FF;" text="Manage Model" />
      <Button fx:id="AfterMenu" layoutX="477.0" layoutY="351.0" mnemonicParsing="false" onAction="#afterMenuButtonOnAction" prefHeight="26.0" prefWidth="96.0" style="-fx-background-color: #00D4FF;" text="Continue" />
      <Button fx:id="SelectButton" layoutX="218.0" layoutY="267.0" mnemonicParsing="false" onAction="#selectButtonOnAction" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #00D4FF;" text="Model Selection" />
      <ImageView fitHeight="113.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../wastewater/Menu.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
